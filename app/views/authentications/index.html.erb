<h1>Listing authentications</h1>

<table>
  <tr>
    <th>User</th>
    <th>Provider</th>
    <th>Uid</th>
    <th>Auth token</th>
    <th>Auth secret</th>
    <th>User name</th>
  </tr>

<% @authentications.each do |authentication| %>
  <tr>
    <td><%=h authentication.user_id %></td>
    <td><%=h authentication.provider %></td>
    <td><%=h authentication.uid %></td>
    <td><%=h authentication.auth_token %></td>
    <td><%=h authentication.auth_secret %></td>
    <td><%=h authentication.user_name %></td>
    <td><%= link_to 'Show', authentication %></td>
    <td><%= link_to 'Edit', edit_authentication_path(authentication) %></td>
    <td><%= link_to 'Destroy', authentication, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New authentication', new_authentication_path %>