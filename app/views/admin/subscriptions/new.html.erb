<%- @title = "New Subscription"; @page_id = "News Letters" -%>
<%- @breadcrumbs = "News Letters &raquo; #{link_to "Subscriptions", admin_subscriptions_path} &raquo; #{@title}" -%>

<div class="inner_content subscriptions" id="admin_subscriptions_new">
  <h2><%= @title %></h2>
  <br />
  <%= form_for :subscription, :url => admin_subscriptions_path  do %>
  	<%= render :partial => "form" %>
  	<p><%= submit_tag "Save" %> or <%= link_to "Cancel", admin_subscriptions_path %></p>
  <% end %>
</div>