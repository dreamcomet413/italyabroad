  <div class="nav_link" style="float: right"><%= link_to 'Back To Mood Page', admin_moods_index_path %></div>
  <br />

<fieldset>
  <legend>Edit Mood</legend>
<%= form_for(@mood, :url => admin_moods_update_path(:id => @mood.id), :html => {:method => :put, :multipart => true}) do |f| %>
